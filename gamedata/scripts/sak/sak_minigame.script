function generate_rand_vector(npc)
local t = {}
local g,l=1,6
  if npc then
  t=xray_vars.load_table(nlc_vars, "dices_npc", {})
  else
  t=xray_vars.load_table(nlc_vars, "dices_gg", {})
  end
  for i = 1,30 do 
    if npc then
    g=lua_random(1,2)
    end
  local p = {lua_random(g,l), lua_random(3-g,l), lua_random(g,l)}
  table.insert (t, p)
  end
  return t
 end
 function generate_rand_mass(npc)
local t = {}
local g,l=1,6
  if npc then
  t=xray_vars.load_table(nlc_vars, "dices_mass_npc", {})
  else
  t=xray_vars.load_table(nlc_vars, "dices_mass_gg", {})
  end
  for i = 1,30 do 
    if npc then
    g=lua_random(1,3)
    end
  local p = {lua_random(g,l), lua_random(4-g,l), lua_random(g,l)}
  table.insert (t, p)
  end
  return t
 end
 function generate_rand_o_mass(npc)
local t = {}
local g,l=1,6
  if npc then
  t=xray_vars.load_table(nlc_vars, "dices_o_mass_npc", {})
  else
  t=xray_vars.load_table(nlc_vars, "dices_o_mass_gg", {})
  end
  for i = 1,30 do 
    if npc then
    g=lua_random(1,3)
    else
    l=lua_random(1,2)
    end
  local p = {lua_random(g,4+l), lua_random(4-g,7-l), lua_random(g,6)}
  table.insert (t, p)
  end
  return t
 end
function roll_dice_npc()
